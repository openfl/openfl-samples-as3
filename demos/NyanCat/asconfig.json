{
    "config": "js",
    "type": "app",
    "compilerOptions": {
        "output": "./",
        "html-template": "template.html",
        "source-path": [
            "src"
        ],
        "external-library-path": [
            "node_modules/openfl/dist/openfl.swc"
        ],
        "source-map": true,
        "targets": [
            "JS"
        ]
    },
    "additionalOptions": [
        "-js-getter-prefix=get_",
        "-js-setter-prefix=set_"
    ],
    "mainClass": "Main"
}
